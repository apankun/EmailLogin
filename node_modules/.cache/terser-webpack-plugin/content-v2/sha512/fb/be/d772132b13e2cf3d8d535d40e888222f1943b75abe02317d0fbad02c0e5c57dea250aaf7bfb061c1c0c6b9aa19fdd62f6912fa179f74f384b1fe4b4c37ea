 from \\\"../components/bbs\\\";\\r\\n    import favorite from \\\"../components/favorite\\\";\\r\\n    import donote from \\\"../components/donote\\\";\\r\\n\\r\\n\\r\\n    export default {\\r\\n        components: {loading,liked,resetpwd,bbs,favorite,donote},\\r\\n        //            var stock=this.$route.params.stock;\\r\\n        data() {\\r\\n            return {\\r\\n                stockId:'',\\r\\n                stockName:'',\\r\\n                stockMarket:'',\\r\\n                isUp:1,//1表示涨，0表示跌\\r\\n                isUpStop:0,//0表示不涨停，1表示涨停\\r\\n                percentage:\\\"\\\",\\r\\n                username:null,\\r\\n                dialogVisible: false,\\r\\n                resestpwdVisible:false,\\r\\n                bbsVisible:false,\\r\\n                favoriteVisible:false,\\r\\n                donateVisible:false,\\r\\n                maps:[]\\r\\n            }\\r\\n        },\\r\\n            mounted:function(){\\r\\n                this.$nextTick(()=>{\\r\\n                    this.getAllStocks()\\r\\n                 //   this.getUserStock()\\r\\n                })\\r\\n                this.username=sessionStorage.getItem('username');\\r\\n                this.stockId=this.$route.query.stockId;\\r\\n                console.log(this.$route.query.szStockId);\\r\\n                this.stockName = this.$route.query.stockName;\\r\\n                this.percentage=this.$route.query.percentage;\\r\\n                this.stockMarket=this.$route.query.stockMarket;\\r\\n                this.isUp=this.$route.query.isUp;\\r\\n                this.isUpStop=this.$route.query.isUpStop;\\r\\n            },\\r\\n        methods:{\\r\\n            getAllStocks(){\\r\\n                this.$api.stock.getAll({}).then(({data})=>{\\r\\n                    if(data.status=='0'){\\r\\n                        var jsonObj=JSON.parse(data.content);\\r\\n                        for (var i = 0; i < jsonObj.length; i++) {\\r\\n                            var record = jsonObj[i];\\r\\n                            var stockId = record['stockId'];\\r\\n                            var stockName = record['stockName'];\\r\\n                            this.maps.push({\\r\\n                                label: stockName,\\r\\n                                key: stockId,\\r\\n                            });\\r\\n                        }\\r\\n\\r\\n                    }else {\\r\\n                        this.$message.error(data.message);\\r\\n                    }\\r\\n                })\\r\\n            },\\r\\n            addLikedStock(stockId)\\r\\n            {\\r\\n                //let stockNum=[];\\r\\n                //stockMap.push({stockId});\\r\\n                //add likedStock\\r\\n                this.$api.stock.addLikedStock({//post请求发送到searchStock\\r\\n                    stockNum:stockId+\\\",\\\",\\r\\n                    email:sessionStorage.getItem('username')\\r\\n                }).then((res)=>{\\r\\n                    if(res.data.status=='0'){\\r\\n                        this.$alert(\\\"添加成功\\\");\\r\\n\\r\\n                    }else {\\r\\n                        this.$message.error(res.data.message);\\r\\n                    }\\r\\n\\r\\n                })\\r\\n            },\\r\\n            delLikedStock(stockId)\\r\\n            {\\r\\n                //del likedStock\\r\\n                this.$api.stock.delLikedStock({//post请求发送到searchStock\\r\\n                    stockNum:stockId+\\\"\\\",\\r\\n                    email:sessionStorage.getItem('username')\\r\\n                }).then((res)=>{\\r\\n                    if(res.data.status=='0'){\\r\\n                        this.$alert(\\\"删除成功\\\");\\r\\n\\r\\n                    }else {\\r\\n                        this.$message.error(res.data.message);\\r\\n                    }\\r\\n\\r\\n                })\\r\\n            },\\r\\n            handleClose(done) {\\r\\n                this.$confirm('确认关闭？')\\r\\n                    .then(_ => {\\r\\n                        done();\\r\\n                    })\\r\\n                    .catch(_ => {});\\r\\n            },\\r\\n            back(){\\r\\n                this.$router.go(-1);//返回上一层\\r\\n            },\\r\\n            logout(){\\r\\n                this.$store.dispatch('UserLogout')\\r\\n                if(! this.$store.state.token){\\r\\n\\r\\n                    this.$router.go(0)\\r\\n                }else{\\r\\n                    this.$message.error('退出失败');\\r\\n                }\\r\\n\\r\\n            }\\r\\n        }\\r\\n\\r\\n\\r\\n\\r\\n\\r\\n    }\\r\\n</script>\\r\\n\\r\\n<style lang='scss' scoped>\\r\\n\\r\\n    @media (max-width: 2000px)  {//1920x1080\\r\\n        .show{\\r\\n            position:relative;\\r\\n            width: 100%;\\r\\n            height: 100%;\\r\\n           // background-image: url(../common/img/background.jpg);\\r\\n            background-size:100% 100%;\\r\\n            display: flex;\\r\\n            flex-direction: column;\\r\\n            align-items: center;\\r\\n            justify-content: center;\\r\\n            color: #fff;\\r\\n            .logo{\\r\\n                //  margin-bottom:10%;\\r\\n                //margin-left:15%;\\r\\n                display: flex;\\r\\n                flex-direction: column;\\r\\n                align-items: center;\\r\\n                justify-content: center;\\r\\n                img{\\r\\n                    width: 60%;\\r\\n                }\\r\\n            }\\r\\n            .header{\\r\\n                position:absolute;\\r\\n                right:10px;\\r\\n                top:5px;\\r\\n            }\\r\\n            .donate{\\r\\n                position:absolute;\\r\\n                right:20px;\\r\\n                top:20px;\\r\\n\\r\\n            }\\r\\n            .show-echarts {\\r\\n                width: 50%;\\r\\n                height:30%;\\r\\n                border-radius: 30px;\\r\\n                box-shadow: 0 2px 4px rgba(0, 0, 0, .12), 0 0